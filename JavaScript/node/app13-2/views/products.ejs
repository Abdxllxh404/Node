<!DOCTYPE html>
<html lang="th">
    <%- include('layouts/head') %>
    <style>
        td {
            padding: 5px;
        }
    </style>
<body>
    <%- include('layouts/nav') %>
    <br>
    <div id="content">
        <table border="1" style="margin: auto">
            <tr><th>name</th><th>price</th><th>color</th></tr>
            <% for (i in products) { %>
            <tr>
                <% for (p in products[i]) { %>
                    <td><%= products[i][p] %></td>
                <% } %>
            </tr>
            <% } %>
        </table>
    </div>

    <%- include('layouts/footer') %>
</body>
</html>